@import "./styles/Colors.less";

@import "./fonts/fonts.less";
@import "~antd/dist/antd.dark.less";

@import "./styles/Antd.less";
@import "./styles/Utils.less";
@import "./styles/Layout.less";
@import "./styles/Typography.less";

@import "./styles/User.less";
@import "./styles/FullscreenImage.less";

.ant-image,
.ant-image-size,
.ant-image-img {
	border-radius: 8px;
	height: 100%;
	// border: inset 1px @nero solid;
	// margin-bottom: 4px;

	@media screen and (min-width: 720px) {
		// margin-bottom: 28px;
	}
}

body {
	background-color: @nero;
	color: @offwhite;

	@media screen and (max-width: 719px) {
		font-size: 0.9em;
		primary-color: #b3ff00;
	}
}

* {
	word-spacing: -4px;
}

// scrollbar

::-webkit-scrollbar {
	width: 10px;
}

/* Track */
::-webkit-scrollbar-track {
	background: @lightgrey;
	border-radius: 8px;
}

/* Handle */
::-webkit-scrollbar-thumb {
	background: @offwhite;
	border-radius: 8px;
}

// -----

::selection {
	color: @offwhite;
	background: @eclipse;
}

a {
	color: @offwhite;
	a:hover {
		color: @offwhite;
	}
	a:active {
		color: @offwhite;
	}
}
a:hover {
	color: white;
}

h2 {
	color: rgba(252, 252, 243, 1);
	margin: 0;
}

p {
	margin: 0;
}

.nav-item-logo-left-top {
	padding: 0;
}

.logo-left-top-text {
	display: none;
	font-weight: bold;
	color: @offwhite;
	width: 100%;
	&visited {
		color: @offwhite;
	}
	// @media screen and (min-width: 720px) {
	//   display: block;
	// }
}

.logo-left-top {
	width: 100%;
	height: 64px;
	display: block;
	.anticon {
		font-size: 41px;
		margin-top: 10px;
	}
	// @media screen and (min-width: 720px) {
	//   display: none;
	// }
}

@import "./styles/Grids";
@import "./styles/Colors";

.nl-white-box {
	max-width: calc(100vw - 18px);
	border-radius: 12px;
	background: #959595;
	padding: 9px;
	margin: 0px;
	@media screen and (min-width: 576px) {
		padding: 18px;
		margin: 0px;
		padding: 36px;
	}
}

.ant-modal-wrap {
	background-color: @nero_80_opacity;
}

.nl-white-box-modal {
