{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "React Library",
  "extends": "./base.json",
  "compilerOptions": {
    "lib": [
      "ES2015"
    ],
    "module": "ESNext",
    "target": "ES6",
    "jsx": "react-jsx",
    "rootDir": "src",
    "baseUrl": ".",
    "outDir": "dist",
    "declaration": true,
    "declarationMap": true,
    "declarationDir": "dist",
    "composite": true,
    "paths": {}
  }
}